{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning",
            "Microsoft.EntityFrameworkCore.Database.Command": "Warning"
        }
    },

    "ConnectionStrings": {
        "MetaDbMsSqlConnection": "{MSSQL_CONNECTION_STRING}"
    },

    "VirtualFit": {
        "ApiToken": "{API_TOKEN}"
    },

    "Jwt": {
        "Key": "{JWT_TOKEN}",
        "Issuer": "authorization-server",
        "Audience": "dvv153",
        "ExpiresDuration": "00:10:00"
    },

    "GoogleAuth": {
        "ClientId": "{GOOGLE_CLIENT_ID}",
        "ClientSecret": "{GOOGLE_SECRET}",
        "ApplicationName": "virtual-fit",
        "RedirectUrl": "https://176.108.255.113:5000/api/google-auth/callback"
    },

    "VkAuth": {
        "ClientId": "53137675",
        "RedirectUrl": "https://176.108.255.113:5000/api/vk-auth/callback",
        "Scope": "email phone"
    },

    "YandexAuth": {
        "ClientId": "{YANDEX_CLIENT_ID}",
        "ClientSecret": "{YANDEX_CLIENT_SECRET}",
        "RedirectUrl": "https://176.108.255.113:5000/api/yandex-auth/callback"
    },

    "MailRuAuth": {
        "ClientId": "{MAIL_CLIENT_ID}",
        "ClientSecret": "{MAIL_CLIENT_SECRET}",
        "RedirectUrl": "https://176.108.255.113:5000/api/mailru-auth/callback",
        "Scope": "userinfo email"
    },

    "SmtpConfig": {
        "Host": "smtp.mail.ru",
        "Port": 465,
        "Username": "{USERNAME}",
        "Password": "{SMTP_PASSWORD}"
    },

    "GoogleRecaptcha": {
        "SecretKeyForOxfordAp": "{SECRET_KEY_FOR_OXFORD_AP}",
        "SecretKeyForYashelCenter": "{SECRET_KEY_FOR_YASHEL_CENTER}",
        "SecretKeyForPingmetasks": "{SECRET_KEY_FOR_PINGMETASKS}"
    },

    "AiClientConfig": {
        "OpenAiApiKey": "{OPEN_AI_API_KEY}"
    },

    "ProxySettings": {
        "Enabled": true,
        "Ip": "192.168.1.100",
        "Port": 8080,
        "Login": "{PROXY_LOGIN}",
        "Password": "{PROXY_PASSWORD}"
    }
}
